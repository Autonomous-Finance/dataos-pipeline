# Prefect Configuration
PREFECT_API_URL=https://api.prefect.cloud/api/accounts/<your-account-id>/workspaces/<your-workspace-id>
PREFECT_API_KEY=<your-prefect-api-key>
DEPLOYMENT_NAME=my-deployment

# ClickHouse Configuration
CLICKHOUSE_HOST=<your-clickhouse-host>
CLICKHOUSE_PORT=9000
CLICKHOUSE_USER=<your-clickhouse-user>
CLICKHOUSE_PASSWORD=<your-clickhouse-password>
CLICKHOUSE_DB=<your-database-name>

# OpenAI Configuration (for embeddings and text generation)
OPENAI_API_KEY=<your-openai-api-key>

# GPU Configuration (optional)
FORCE_GPU=no
